<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="yu.mapper.StoreMapper">

    <update id="update" parameterType="yu.model.Order">
        update store set store=store-#{num} where product_id=#{productId}
    </update>
</mapper>